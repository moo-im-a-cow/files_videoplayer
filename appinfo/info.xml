<?xml version="1.0"?>
<info>
	<id>plyr_videoplayer</id>
	<name>Plyr Video player</name>
	<description>
	Modification of the official video player(https://github.com/nextcloud/files_videoplayer)
	Uses Plyr (https://github.com/Selz/plyr) instead of videojs
	
	</description>
	<version>1.0.0</version>
	<licence>MIT</licence>
	<author>Robert K</author>
	<default_enable/>
	<documentation>
		<user>https://github.com/owncloud/files_videoplayer/blob/master/README.md</user>
		<admin>https://github.com/owncloud/files_videoplayer/blob/master/README.md</admin>
		<developer>https://github.com/owncloud/files_videoplayer/blob/master/README.md</developer>
	</documentation>
	<website>https://github.com/moo-im-a-cow/plyr_videoplayer</website>
	<bugs>https://github.com/moo-im-a-cow/plyr_videoplayer/issues</bugs>
	<repository type="git">https://github.com/moo-im-a-cow/plyr_videoplayer.git</repository>
	<dependencies>
		<nextcloud min-version="11" max-version="11" />
	</dependencies>
</info>
